		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"postinstall": "husky install"
	},
	"lint-staged": {
		"**/*.{ts,js,svelte}": [
			"prettier --write",
			"git add"
		]
